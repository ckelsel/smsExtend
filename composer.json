{
    "name":"ckelsel/sms-extend",
    "description":"This is just an extension package for sending SMS",
    "keywords":["sms", "laravel", "php", "message"],
    "type":"library",
    "require":{
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev":{
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Len\\SmsExtend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Len\\SmsExtend\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors":[
        {
            "name":"len",
            "email":"wcf818520@sina.com"
        }
    ],
    "extra":{
        "laravel":{
            "providers":[
                "Len\\SmsExtend\\Providers\\SmsExtendServiceProvider"
            ],
            "aliases":{
                "SmsExtend":"Len\\SmsExtend\\Facades\\SmsExtend"
            }
        }
    }
}
